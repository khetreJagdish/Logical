package com.exception;

public class ExceptionDemo
{
	public static void main(String[] args) 
	{
		doStuff();
	}

	private static void doStuff() 
	{
		doMoreStuff();
		
	}

	private static void doMoreStuff() 
	{
		try
		{
			Thread.sleep(1000);
		} catch (InterruptedException e)
		{
			// TODO: handle exception
		}
		
		
	}
}
